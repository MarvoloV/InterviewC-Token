  "version": "1.0.0",
  "scripts": {
    "dev": "npx serverless offline start --reloadHandler",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "devDependencies": {
