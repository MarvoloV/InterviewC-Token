    "@types/validator": "^13.11.5",
    "jest": "^29.7.0",
    "prisma": "^5.5.2",
    "serverless": "^3.21.0",
    "serverless-offline": "^9.3.1",
    "serverless-plugin-typescript": "^2.1.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "redis": "^4.6.10",
    "validator": "^13.11.0"
  }
